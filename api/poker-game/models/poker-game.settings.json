{
  "kind": "collectionType",
  "collectionName": "poker_games",
  "info": {
    "name": "Poker Game",
    "description": ""
  },
  "options": {
    "increments": true,
    "timestamps": true,
    "draftAndPublish": false
  },
  "attributes": {
    "User": {
      "plugin": "users-permissions",
      "model": "user"
    },
    "Hand": {
      "type": "json"
    },
    "Deck": {
      "type": "json",
      "required": false,
      "private": true
    },
    "Done": {
      "type": "boolean",
      "default": false,
      "required": true
    },
    "Holds": {
      "type": "json",
      "required": false
    },
    "Draw": {
      "type": "json"
    },
    "FinalCards": {
      "type": "json"
    },
    "Result": {
      "type": "json"
    },
    "Hash": {
      "type": "uid",
      "required": false
    },
    "Strategy": {
      "type": "json"
    },
    "Mode": {
      "type": "enumeration",
      "enum": [
        "classic",
        "casual",
        "trainer"
      ],
      "required": true,
      "default": "classic"
    }
  }
}
